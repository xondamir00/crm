NODE_ENV=development
PORT=3000
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/crm_db?schema=public
JWT_ACCESS_SECRET=supersecret
JWT_ACCESS_EXPIRES=15m
JWT_REFRESH_SECRET=supersecretrefresh
JWT_REFRESH_EXPIRES=7d

# 1. Eski container/volume'larni tozalash
docker compose down -v --remove-orphans

# 2. (Ixtiyoriy) qayta build – Postgres image uchun shart emas, lekin zarar ham yo‘q
docker compose build --no-cache

# 3. To‘g‘ri service nomi bilan DB'ni ko'tarish
docker compose up -d postgres

# 4. DB ko'tarilganini tekshir
docker ps   # crm_postgres, PORTS: 0.0.0.0:5432->5432/tcp bo'lishi kerak

# 5. Node / Prisma tarafni tozalash
rm -rf node_modules prisma/.cache

npm i
npx prisma generate

# 6. Migratsiya
npx prisma migrate reset --force


